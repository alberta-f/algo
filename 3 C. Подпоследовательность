def main():
    s, t = input(), input()
    s_index, t_index = 0, 0
    counter = 0

    while s_index < len(s) and t_index < len(t):
        if s[s_index] == t[t_index]:
            counter += 1
            s_index += 1
        
        t_index += 1

    print(counter == len(s))

if __name__ == '__main__':
    main()
